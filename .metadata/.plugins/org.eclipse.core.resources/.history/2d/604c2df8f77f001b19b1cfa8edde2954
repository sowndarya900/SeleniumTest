/** page object encapsulates the Homepage
 * @author data about locators of login page
 *
 */
@Test
public class HomePage {
	   WebDriver driver;

	  public HomePage(WebDriver driver,String url){
	    this.driver = driver;
	    driver.get(url);
	    driver.manage().window().maximize();
	    
	
	  }
}